        $(document).ready(function(){
            $(".dropdown").hover(
                    function() {
                        $('.dropdown-menu', this).not('.in .dropdown-menu').stop(true,true).show();
                        $(this).toggleClass('open');
                    },
                    function() {
                        $('.dropdown-menu', this).not('.in .dropdown-menu').stop(true,true).hide();

                    }
            );
        });
