                                    <td><?php echo e(config('app.currency')); ?><?php echo e(number_format($comissionInvoice->refer_agency_amount,2)); ?></td>
                                    <td><?php echo e(config('app.currency')); ?><?php echo e(number_format($comissionInvoice->paid,2)); ?><?php if(!empty($comissionInvoice->paid)): ?>
                                            <em></em><?php endif; ?></td>
                                    <td><?php echo e(config('app.currency')); ?><?php echo e(number_format($comissionInvoice->refer_agency_bal,2)); ?></td>
                                </tr>
                            <?php endforeach; ?>
                            <?php endif; ?>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer clearfix">
